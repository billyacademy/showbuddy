<%= @concert.artist_1 %><br>
<%= @concert.date %><br>
<%= @concert.time %><br>



<% if @rsvp.empty? %>
  <%= button_to "I want to go!", concert_rsvps_path(params[:id]) %>
<% else %>
  <%= "You have already RSVP'd for this concert!" %>
  <h1> Others who want to go see <%= @concert.artist_1 %>: </h1>
  <% @rsvps.each do |rsvp| %>
    <% if rsvp.concert_id == params[:id].to_i && rsvp.user_id != current_user.id %>
      <%= rsvp.user.name %>
    <% end %>
  <% end %>
<% end %>
