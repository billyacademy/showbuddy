<div class="row">
  <div class="small-3 small-centered columns">
    <div class="transbox2">
      <%= form_for [:admin, @venue] do |f|%>
      <div>
        <%= f.label :name %>
        <%= f.text_field :name %>
      </div>

      <div>
        <%= f.label :address_line_1 %>
        <%= f.text_field :address_line_1 %>
      </div>

      <div>
        <%= f.label :address_line_2 %> *optional
        <%= f.text_field :address_line_2 %>
      </div>

      <div>
        <%= f.label :city %>
        <%= f.text_field :city %>
      </div>

      <div>
        <%= f.label :state %>
        <%= f.text_field :state %>
      </div>

      <div>
        <%= f.label :zip %>
        <%= f.text_field :zip %>
      </div>

      <div>
        <%= f.label :website %>
        <%= f.text_field :website %>
      </div>

      <div>
        <%= f.button "Submit"%>
      </div>

      <% end %>
    </div>
  </div>
</div>
